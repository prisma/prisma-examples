{
  "name": "prisma-file-handling-s3",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -- nodemon -x ts-node -e ts,graphql src/index.ts",
    "debug": "dotenv -- nodemon -x 'ts-node --inspect' -e ts,graphql src/index.ts",
    "playground": "dotenv -- graphql playground",
    "bind": "graphql prepare",
    "build": "rm -rf dist && tsc"
  },
  "dependencies": {
    "aws-sdk": "2.350.0",
    "express": "4.16.4",
    "graphql": "0.13.2",
    "graphql-import": "0.7.1",
    "graphql-yoga": "1.16.7",
    "mime-types": "2.1.21",
    "multiparty": "4.2.1",
    "prisma-binding": "2.1.6",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.17.0",
    "nodemon": "1.18.6",
    "prisma": "1.20.0",
    "ts-node": "7.0.1",
    "typescript": "3.1.6"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
