{
  "name": "prisma-file-handling-s3",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "dotenv -- nodemon -x ts-node -e ts,graphql src/index.ts",
    "debug": "dotenv -- nodemon -x 'ts-node --inspect' -e ts,graphql src/index.ts",
    "playground": "dotenv -- graphql playground",
    "bind": "graphql prepare",
    "build": "rm -rf dist && tsc"
  },
  "dependencies": {
    "aws-sdk": "2.324.0",
    "express": "4.16.3",
    "graphql": "0.13.2",
    "graphql-import": "0.7.1",
    "graphql-yoga": "1.16.2",
    "mime-types": "2.1.20",
    "multiparty": "4.2.1",
    "prisma-binding": "1.5.19",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "dotenv-cli": "1.4.0",
    "graphql-cli": "2.16.7",
    "nodemon": "1.18.4",
    "prisma": "1.17.1",
    "ts-node": "4.1.0",
    "typescript": "2.9.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
