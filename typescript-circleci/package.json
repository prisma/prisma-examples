{
  "name": "prisma-circleci",
  "version": "1.0.0",
  "description": "Prisma CircleCI example",
  "author": "Waldemar Penner",
  "license": "MIT",
  "scripts": {
    "test": "jest --forceExit --detectOpenHandles",
    "prisma": "cd prisma && prisma"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "prisma": "^1.17.1",
    "ts-jest": "^23.10.3",
    "typescript": "^3.1.1"
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".test.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "graphql-tools": "^4.0.0"
  }
}
