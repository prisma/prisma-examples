{
  "name": "prisma-client-examples",
  "version": "1.0.0",
  "repository": "git@github.com:prisma/prisma-client-js.git",
  "license": "MIT",
  "scripts": {
    "test": "yarn prettier-check && yarn build-readmes",
    "build-readmes": "inline-markdown --input ../readmes --output ../../",
    "dev": "watch 'yarn run build-readmes' ../readmes",
    "prettier-check": "prettier-check --config .prettierrc \"../../{javascript,typescript}/*/{src,test,prisma}/**/*.{ts,js}\"",
    "prettier-fix": "prettier --config .prettierrc --write \"../../{javascript,typescript}/*/{src,test,prisma}/**/*.{ts,js}\""
  },
  "devDependencies": {
    "inline-markdown": "0.1.6",
    "prettier": "2.5.1",
    "prettier-check": "2.0.0",
    "watch": "1.0.2"
  }
}
